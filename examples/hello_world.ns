putchar(c: char): int {
    #asm "mov %si, %bp"
    #asm "mov %cx, >-c"
    #asm "add %si, %cx"
    #asm "mov %ax, 1"
    #asm "mov %di, 1"
    #asm "mov %dx, 1"
    #asm "syscall"
    return c;
}

main(): int{
    len: int = 13;
    str: char[] = "Hello, World!";
    i: int = 0;
    while (i < len) {
        putchar(str[i]);
        i = i + 1;
    }
    return 0;
}