int putchar(char c) {
    #asm "mov %si, %bp"
    #asm "mov %cx, >-c"
    #asm "add %si, %cx"
    #asm "mov %ax, 1"
    #asm "mov %di, 1"
    #asm "mov %dx, 1"
    #asm "syscall"
    return c;
}

int main() {
    int len = 13;
    char[] str = "Hello, World!";
    int i = 0;
    while (i < len) {
        putchar(str[i]);
        i = i + 1;
    }
    return 0;
}